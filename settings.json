{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "editor.minimap.enabled": true,
  "editor.wordWrap": "bounded",
  "workbench.colorCustomizations": {},
  "liveServer.settings.donotShowInfoMsg": true,
  "C_Cpp.updateChannel": "Insiders",
  "window.zoomLevel": 0,
  "liveServer.settings.port": 0,
  "liveServer.settings.donotVerifyTags": true,
  "todohighlight.keywords": [{
      "text": "WARNING",
      "color": "#FFD76B",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "WARN",
      "color": "#FFD76B",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "FIXME",
      "color": "#F07178",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "DEBUGGING",
      "color": "#F07178",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "DEPRECIATED",
      "color": "#F78C6C",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "DEBUG",
      "color": "#F07178",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "REVIEW",
      "color": "#ACE88D",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "NOTE",
      "color": "white",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    },
    {
      "text": "TODO",
      "color": "#ACE88D",
      "backgroundColor": "transparent",
      "isWholeLine": false,
      "fontWeight": "bold",
    }
  ],
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "cSpell.enableFiletypes": [
    "!cpp",
    "!css",
    "!html",
    "!javascript",
    "!javascriptreact",
    "!jsonc",
    "!scss"
  ],
  "cSpell.userWords": [
    "PWAMP",
    "Proxying",
    "Zales",
    "adyar",
    "amphtml",
    "amplist",
    "amplists",
    "coronavirus",
    "crossorigin",
    "keychain",
    "linebreak",
    "msrp",
    "pinterest",
    "typeof",
    "womp",
    "wompme"
  ],
  "markdownlint.config": {
    "default": true,
    "MD026": {
      "punctuation": ""
    }
  },
  "editor.wordWrapColumn": 150,
  "workbench.colorTheme": "Material Custom",
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
    // "editor.defaultFormatter": "HookyQR.beautify"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.alwaysShowStatus": true,
  "editor.minimap.renderCharacters": false,
  "files.insertFinalNewline": true,
  "editor.largeFileOptimizations": false,
}
